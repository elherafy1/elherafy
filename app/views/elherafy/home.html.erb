<div class="col-sm-12">
 <div id="myCarousel" class="carousel slide" data-ride="carousel">

      <!-- Indicators -->
      <ol class="carousel-indicators">
        <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
        <li data-target="#myCarousel" data-slide-to="1"></li>
        <li data-target="#myCarousel" data-slide-to="2"></li>
      </ol>
      <div class="carousel-inner" role="listbox">
        <div class="item active">
          <img src="data:image/gif;base64,R0lGODlhAQABAIAAAGZmZgAAACH5BAAAAAAALAAAAAABAAEAAAICRAEAOw==" alt="First slide">
          <div class="container">
            <div class="carousel-caption">
<h1><%= Worker.max_mabalat.name %></h1>
              <% if Worker.max_mabalat.average_rate > 20 %>
                <td><img src="http://www.clker.com/cliparts/Z/X/M/T/C/s/led-circle-green-md.png" width="90" height="90"></td>
              <% elsif Worker.max_mabalat.average_rate > 15 && Worker.max_mabalat.average_rate <= 20%>
                <td><img src="http://www.clker.com/cliparts/z/Y/Q/v/w/e/led-circle-yellow-md.png" width="90" height="90"></td>
              <% elsif Worker.max_mabalat.average_rate > 10 && Worker.max_mabalat.average_rate <= 15 %>
                <td><img src="http://www.clker.com/cliparts/6/y/a/i/z/9/led-circle-orange-md.png" width="90" height="90"></td>
              <% else %>
                <td><img src="http://www.clker.com/cliparts/T/t/g/p/t/5/led-circle-red-md.png" width="90" height="90"></td>            
              <% end %>

              <p><strong>MABALT</strong></p>
              <p>
              <%= render 'elherafy/home', :worker => Worker.max_mabalat%>
              </p><br>              
              <p></p>
              <p><a <%= link_to 'Show Profile', Worker.max_mabalat , :class => "btn btn-primary"%></a></p>
            </div>
          </div>
        </div>
        <div class="item">
          <img src="data:image/gif;base64,R0lGODlhAQABAIAAAGZmZgAAACH5BAAAAAAALAAAAAABAAEAAAICRAEAOw==" alt="Second slide">
